{
	"name": "studiomitte/solr-enhancement",
	"type": "typo3-cms-extension",
	"keywords": [
		"TYPO3",
		"extension",
		"solr"
	],
	"authors": [
		{
			"name": "Georg Ringer",
			"role": "Developer",
			"homepage": "https://studiomitte.com"
		}
	],
	"support": {
		"issues": "https://github.com/studiomitte/solr-enhancement/issues",
		"source": "https://github.com/studiomitte/solr-enhancement"
	},
	"description": "Small enhancements for solr extension",
	"homepage": "https://studiomitte.com",
	"license": [
		"GPL-2.0-or-later"
	],
	"require": {
		"typo3/cms-core": "^9.5 || ^10.4 || ^11.5",
		"apache-solr-for-typo3/solr": "^10 || ^11"
	},
	"autoload": {
		"psr-4": {
			"StudioMitte\\SolrEnhancement\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"StudioMitte\\SolrEnhancement\\Tests\\": "Tests"
		}
	},
	"replace": {
		"studiomitte/solr_enhancement": "self.version",
		"typo3-ter/solr_enhancement": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "solr_enhancement"
		}
	}
}
